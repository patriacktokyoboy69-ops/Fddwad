-- ğŸš€ MAIN EXECUTION
-- ================================================================
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("ğŸš€ Tutorial Complete System v2.0")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")

-- à¹€à¸£à¸´à¹ˆà¸¡à¸£à¸°à¸šà¸šà¸à¸·à¹‰à¸™à¸à¸²à¸™
setupDragonSystem()
setupAutoFlyControl()

print("â³ à¸£à¸­à¸£à¸°à¸šà¸šà¹‚à¸«à¸¥à¸” 3 à¸§à¸´...")
task.wait(3)

-- à¸•à¸±à¸”à¸ªà¸´à¸™à¹ƒà¸ˆà¸§à¹ˆà¸²à¸ˆà¸°à¸—à¸³à¸­à¸°à¹„à¸£à¸•à¹ˆà¸­
local action, data = determineNextAction()

print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("ğŸ¯ à¸à¸²à¸£à¸”à¸³à¹€à¸™à¸´à¸™à¸à¸²à¸£:", action)
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")

if action == "TUTORIAL" then
	-- à¸¡à¸µ Nest à¹à¸¥à¹‰à¸§ â†’ à¸—à¸³ Tutorial à¸›à¸à¸•à¸´
	print("âœ… à¸à¸š Nest â†’ à¸”à¸³à¹€à¸™à¸´à¸™à¸à¸²à¸£ Tutorial")
	
	for i = 1, 5 do
		print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
		print("ğŸ” à¸£à¸­à¸šà¸—à¸µà¹ˆ", i)
		print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
		
		completeEggTutorial()
		task.wait(0.1)
		completeIncubatorTutorial()
		task.wait(0.1)
		completeGrowDragonTutorial()
		task.wait(0.1)
		completeFlyingTutorialStage()
		task.wait(0.1)
		completeSecondIncubatorTutorial()
		task.wait(0.1)
	end
	
	print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
	print("âœ… Tutorial à¹€à¸ªà¸£à¹‡à¸ˆà¸ªà¸¡à¸šà¸¹à¸£à¸“à¹Œ!")
	print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
	
	-- à¸§à¸²à¸£à¹Œà¸›à¸à¸¥à¸±à¸š Lobby
	print("ğŸŒ à¸§à¸²à¸£à¹Œà¸›à¹„à¸› Lobby...")
	task.wait(3)
	safeInvoke(Remotes.WorldTeleportRemote, 3475397644, {})
	
elseif action == "QUESTS" then
	-- à¸—à¸³à¹€à¸„à¸§à¸ªà¹ƒà¸«à¹‰à¹€à¸ªà¸£à¹‡à¸ˆà¸à¹ˆà¸­à¸™
	print("ğŸ¯ à¸—à¸³à¹€à¸„à¸§à¸ª Lobby à¹ƒà¸«à¹‰à¹€à¸ªà¸£à¹‡à¸ˆ")
	local questSuccess = completeLobbyQuests()
	
	if questSuccess then
		print("âœ… à¹€à¸„à¸§à¸ªà¹€à¸ªà¸£à¹‡à¸ˆ â†’ à¸¥à¸­à¸‡à¸£à¸±à¸™ Fixscript")
		
		if runFixscript() then
			task.wait(5)
			
			-- à¸•à¸£à¸§à¸ˆà¸ªà¸­à¸š Nest à¸­à¸µà¸à¸„à¸£à¸±à¹‰à¸‡
			local nest = waitForNestWithTimeout(10)
			
			if nest then
				print("âœ… à¸à¸š Nest à¸«à¸¥à¸±à¸‡ Fixscript â†’ à¹€à¸£à¸´à¹ˆà¸¡ Tutorial")
				
				for i = 1, 5 do
					print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
					print("ğŸ” à¸£à¸­à¸šà¸—à¸µà¹ˆ", i)
					print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
					
					completeEggTutorial()
					task.wait(0.1)
					completeIncubatorTutorial()
					task.wait(0.1)
					completeGrowDragonTutorial()
					task.wait(0.1)
					completeFlyingTutorialStage()
					task.wait(0.1)
					completeSecondIncubatorTutorial()
					task.wait(0.1)
				end
				
				print("âœ… Tutorial à¹€à¸ªà¸£à¹‡à¸ˆà¸ªà¸¡à¸šà¸¹à¸£à¸“à¹Œ!")
				
				print("ğŸŒ à¸§à¸²à¸£à¹Œà¸›à¹„à¸› Lobby...")
				task.wait(3)
				safeInvoke(Remotes.WorldTeleportRemote, 3475397644, {})
			else
				warn("âŒ à¸¢à¸±à¸‡à¹„à¸¡à¹ˆà¸à¸š Nest à¸«à¸¥à¸±à¸‡ Fixscript")
				warn("ğŸ”„ à¸à¸£à¸¸à¸“à¸²à¸£à¸µà¸ˆà¸­à¸¢à¸™à¹Œà¹€à¸à¸¡")
			end
		else
			warn("âŒ Fixscript à¸¥à¹‰à¸¡à¹€à¸«à¸¥à¸§")
		end
	else
		warn("âŒ à¸—à¸³à¹€à¸„à¸§à¸ªà¹„à¸¡à¹ˆà¸ªà¸³à¹€à¸£à¹‡à¸ˆ")
	end
	
elseif action == "FIXSCRIPT" then
	-- à¹€à¸„à¸§à¸ªà¸„à¸£à¸šà¹à¸¥à¹‰à¸§ â†’ à¸£à¸±à¸™ Fixscript à¸—à¸±à¸™à¸—à¸µ
	print("âœ… à¹€à¸„à¸§à¸ªà¸„à¸£à¸š â†’ à¸£à¸±à¸™ Fixscript à¸—à¸±à¸™à¸—à¸µ")
	
	if runFixscript() then
		task.wait(5)
		
		local nest = waitForNestWithTimeout(10)
		
		if nest then
			print("âœ… à¸à¸š Nest â†’ à¹€à¸£à¸´à¹ˆà¸¡ Tutorial")
			
			for i = 1, 5 do
				print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
				print("ğŸ” à¸£à¸­à¸šà¸—à¸µà¹ˆ", i)
				print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
				
				completeEggTutorial()
				task.wait(0.1)
				completeIncubatorTutorial()
				task.wait(0.1)
				completeGrowDragonTutorial()
				task.wait(0.1)
				completeFlyingTutorialStage()
				task.wait(0.1)
				completeSecondIncubatorTutorial()
				task.wait(0.1)
			end
			
			print("âœ… Tutorial à¹€à¸ªà¸£à¹‡à¸ˆà¸ªà¸¡à¸šà¸¹à¸£à¸“à¹Œ!")
			
			print("ğŸŒ à¸§à¸²à¸£à¹Œà¸›à¹„à¸› Lobby...")
			task.wait(3)
			safeInvoke(Remotes.WorldTeleportRemote, 3475397644, {})
		else
			warn("âŒ à¹„à¸¡à¹ˆà¸à¸š Nest à¸«à¸¥à¸±à¸‡ Fixscript")
			warn("ğŸ”„ à¸à¸£à¸¸à¸“à¸²à¸£à¸µà¸ˆà¸­à¸¢à¸™à¹Œà¹€à¸à¸¡")
		end
	end
	
else
	-- ERROR
	warn("âŒ à¹€à¸à¸´à¸”à¸‚à¹‰à¸­à¸œà¸´à¸”à¸à¸¥à¸²à¸” - à¹„à¸¡à¹ˆà¸ªà¸²à¸¡à¸²à¸£à¸–à¸”à¸³à¹€à¸™à¸´à¸™à¸à¸²à¸£à¸•à¹ˆà¸­à¹„à¸”à¹‰")
	warn("ğŸ”„ à¸à¸£à¸¸à¸“à¸²à¸•à¸£à¸§à¸ˆà¸ªà¸­à¸šà¹à¸¥à¸°à¸£à¸µà¸ˆà¸­à¸¢à¸™à¹Œà¹€à¸à¸¡")
end

-- à¸›à¸´à¸”à¸£à¸°à¸šà¸š
task.wait(5)
_G.TUTORIAL_SCRIPT_RUNNING = false
print("ğŸ›‘ à¸£à¸°à¸šà¸šà¸«à¸¢à¸¸à¸”à¸—à¸³à¸‡à¸²à¸™")
